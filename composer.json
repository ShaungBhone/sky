{
    "name": "lara-zeus/sky",
    "description": "simple packages to create and manage forms for your users",
    "keywords": [
        "laravel",
        "lara-zeus",
        "sky",
        "cms",
        "pages",
        "posts",
        "menu",
        "news",
        "wordpress laravel",
        "wordpress alternative",
        "laravel cms"
    ],
    "homepage": "https://larazeus.com",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "doctrine/dbal": "^3.5.1",
        "filament/spatie-laravel-media-library-plugin": "^2.16.45",
        "filament/spatie-laravel-tags-plugin": "^2.16.45",
        "filament/spatie-laravel-translatable-plugin": "^2.16.45",
        "lara-zeus/core": "^2.3.4",
        "mohamedsabil83/filament-forms-tinyeditor": "^1.6.4",
        "spatie/laravel-medialibrary": "^10.7.1",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-tags": "^4.3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.11",
        "laravel/pint": "^1.2",
        "nunomaduro/larastan": "^2.2.9",
        "pestphp/pest": "^1.22.2",
        "pestphp/pest-plugin-laravel": "^1.3",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2.2",
        "phpunit/phpunit": "^9.5.26"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Sky\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraZeus\\Sky\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Sky\\SkyServiceProvider"
            ]
        }
    }
}
